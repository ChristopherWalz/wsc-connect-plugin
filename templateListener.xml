<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/templateListener.xsd">
	<import>		
		<templatelistener name="extendedConversationsJS">
			<environment>user</environment>
			<templatename>headIncludeJavaScript</templatename>
			<eventname>javascriptInit</eventname>
			<templatecode><![CDATA[{if MODULE_WSC_CONNECT && (!$__wcf->getUser()->userID || ($__wcf->getUser()->userID && !$__wcf->getUser()->wscConnectToken))}{include file=__wscConnectJS}{/if}]]></templatecode>
		</templatelistener>
		<templatelistener name="extendedConversations">
			<environment>user</environment>
			<templatename>footer</templatename>
			<eventname>footer</eventname>
			<templatecode><![CDATA[{if MODULE_WSC_CONNECT && (!$__wcf->getUser()->userID || ($__wcf->getUser()->userID && !$__wcf->getUser()->wscConnectToken))}{include file=__wscConnect}{/if}]]></templatecode>
		</templatelistener>
	</import>
</data>
