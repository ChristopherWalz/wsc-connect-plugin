<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/templateListener.xsd">
	<import>
		<templatelistener name="extendedConversationsJSInclude">
			<environment>user</environment>
			<templatename>headInclude</templatename>
			<eventname>javascriptInclude</eventname>
			<templatecode><![CDATA[{if MODULE_WSC_CONNECT && WSC_CONNECT_APP_ID && WSC_CONNECT_APP_SECRET && (!$__wcf->getUser()->userID || ($__wcf->getUser()->userID && !$__wcf->getUser()->wscConnectToken))}<script data-relocate="true" src="{@$__wcf->getPath('wcf')}js/CW.WSCConnect.js"></script>{/if}]]></templatecode>
		</templatelistener>
		<templatelistener name="extendedConversationsJS">
			<environment>user</environment>
			<templatename>headInclude</templatename>
			<eventname>javascriptInit</eventname>
			<templatecode><![CDATA[{if MODULE_WSC_CONNECT && WSC_CONNECT_APP_ID && WSC_CONNECT_APP_SECRET && (!$__wcf->getUser()->userID || ($__wcf->getUser()->userID && !$__wcf->getUser()->wscConnectToken))}new CW.WSCConnect('{COOKIE_PREFIX}', {WSC_CONNECT_INFO_TIME});{/if}]]></templatecode>
		</templatelistener>
		<templatelistener name="extendedConversations">
			<environment>user</environment>
			<templatename>footer</templatename>
			<eventname>footer</eventname>
			<templatecode><![CDATA[{if MODULE_WSC_CONNECT && WSC_CONNECT_APP_ID && WSC_CONNECT_APP_SECRET && (!$__wcf->getUser()->userID || ($__wcf->getUser()->userID && !$__wcf->getUser()->wscConnectToken))}{include file=__wscConnect}{/if}]]></templatecode>
		</templatelistener>
	</import>
</data>
